from drawbot_skia.drawbot import *

def apple (x,y):
    polygon((x+20, y+20), (x+40, y+10), (x+50, y+20), (x+60, y+10), (x+80, y+20), (x+80, y+70), (x+60, y+80), (x+50, y+80), (x+40, y+80), (x+40, y+70), (x+80, y+110), (x+77, y+112), (x+65, y+100), (x+40, y+120), (x+10, y+120), (x+30, y+110), (x+60, y+95), (x+50, y+63), (x+40, y+80), (x+20, y+70))

for x in range(7):
    for y in range(7):
        apple(x*140, y*140)

saveImage("49apples.pdf")
